					<dt>Største registrerte lengde</dt>
					<dd>3,1 meter</dd>
					<dt>Geografisk distribusjon</dt>
					<dd>Nordatlantern</dd>
					<dt>Les mer</dt>
					<dd><a href="https://no.wikipedia.org/wiki/Kvitnos">Wiki</a></dd>
				</dl>
				<%- partial("/partials/_map.ejs", {zoom: 6, points: "70.662620, 26.817421"}) %>
